package in.co.oop;

public  class Shape {
 
	
	private int  boderWidth=0;
	private String color=null;
	
	public void setColor(String c) {
		color=c;
	}
	public String getColor() {
	  
	  return color;
	  
	  

	}
	public void setBoderWidth(int bw) {
		boderWidth = bw;
	}
	public int getBoderWidth() {
		return boderWidth;
		
	}
} 
	 
	

