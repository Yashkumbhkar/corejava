package in.co.Rayss;

public class String {
 public static void main(String[] args) {
	
	 String[] name= {"yash","pranav"};
         for ( int i = 0; i < name.length; i++) {
        	 
        	 
        	 for ( int j = 0; j < name[i].length(); j++) {
				
        		 
        		 System.out.println(name[i].charAt(j));
			}
			
		}	 
}


}


