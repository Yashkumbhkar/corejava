package in.co.Inharitance;

public class Triangle {
 
	    private int base =0;
	    private int height =0;
		public int getBase() {
			return base;
		}
		public void setBase(int base) {
			this.base = base;
		}
		public int getHeight() {
			return height;
		}
		public void setHeight(int height) {
			this.height = height;
		}
	    
	    
}
