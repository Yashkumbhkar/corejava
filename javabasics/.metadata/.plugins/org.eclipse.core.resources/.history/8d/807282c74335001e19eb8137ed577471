package in.co.oop;

public class Shap {

	 private  String color =null;
	 private  int  boardarWidth=0;
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public int getBoardarWidth() {
		return boardarWidth;
	}
	public void setBoardarWidth(int boardarWidth) {
		this.boardarWidth = boardarWidth;
	}
	 
	  
		 
	 }

