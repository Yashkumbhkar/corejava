package in.co.Rayss;

public class TestmissingNumber {
 public static void main(String[] args) {
	int []a= {1,2,4,6,5,};
	int []b = {1,2,4,5};
	
	int sum =0;
	int sum1 = 0;
	for (int i = 0; i < b.length; i++) {
		sum = sum+ a[i];
		{
			for (int j = 0; j < b.length; j++) {
				sum1 =sum+b[j];
			}
		}
		System.out.println("missing number is= "+ (sum - sum1));
	
		
	}
}
}
