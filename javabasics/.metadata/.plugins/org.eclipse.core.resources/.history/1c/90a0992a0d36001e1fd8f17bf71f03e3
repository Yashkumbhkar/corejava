package in.co.withconstructor;

public class Circle {
   
	
	
	private   int redius= 0;
	public static double PI =3.15; 
	
	
	public Circle( int redius) {
		this.redius= redius ;
		
	}
	public void setRedius(int redius) {
		this.redius = redius ;
		
		
	}
	public int getRedius() {
		
		return redius;
	}
	 
	
}
