package in.co.rays;

public class Test2DArry {

	public static void main(String[] args) {
		int [] [] table = new int [10] [10];
		for (int i = 2; i < table.length; i++) {
			for (int j = 2; j < table.length; j++) {
				table [i][j] =(i+1)*(j+1);
				System.out.print(table[i][j]+"\t");
			}
		}
		System.out.println();
		System.out.print( " ");
	}
}
