	public static void main(String[] args) {
		
		int[] num  = {1,2,3,4,5};
		int sum =0;
		int b = 0 ; 
		float avg =0;
		
		for (int i = 0; i <= num[i]; i++) {
			
			
		
	       sum = num[i]+b;
		 avg = sum/5;
		 
	
			 
		   
			
		
	} 	 System.out.println(avg);
	 
	
		 }

