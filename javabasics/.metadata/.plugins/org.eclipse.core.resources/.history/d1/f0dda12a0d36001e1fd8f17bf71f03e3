package in.co.withconstructor;

public class Tringle {
	
	 private int base=0;
	 private int heigth =0;
	 
	 public Tringle (int base , int heigth ) {
		 
		 this.base = base;
		 this.heigth = heigth ;
		 
	 }
	 public void setBase (int base ) {
		 this.base = base;
		 
	 }
	 public int  getBase() {
		 
		 return base;
	 }
	 public void setheigth ( int heigth ) {
		 this.heigth= heigth ;
		 
	 }
	 
	 public int getHeigth () {
		 
		 return heigth;
	 }

	 
	 
	 
	 
	 
}
