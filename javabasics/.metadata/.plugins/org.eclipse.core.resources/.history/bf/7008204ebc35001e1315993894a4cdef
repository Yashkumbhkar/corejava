package in.co.oop;

public class Tringle  extends Squer  {
  private int angle ; 
   private int length;
public int getAngle() {
	return angle;
}
public void setAngle(int angle) {
	this.angle = angle;
}
public int getLength() {
	return length;
}
public void setLength(int length) {
	this.length = length;
}
}
