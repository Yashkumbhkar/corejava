public class palindrom {
	
	
	public static void main(String[] args) {
		
		
		int a=123;
		
		int b=a;
		int c=0;
		int d;
		
		
		while(b>0) {
			
			d=b%10;
			
			c=c*10+d;
			
			b=b/10;
			
			
		}
		if (a==b) {
			
			System.out.println("yes this is palidrom");
					}else {
						
						
						System.out.println("no this is not palindrom");
