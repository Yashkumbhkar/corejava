package in.co.Rayss;

public class Yash1 {
 public static void main(String[] args) {
	
	 String[] name = {"yash"};

	 
	 for (int i = 0; i < name.length; i++) {
		 
		 
		 for (int j = 0; j < name[i].length(); j++) {
			 
			 
			 System.out.print(name[i].charAt(0));
			
		}
		 
		 
		
	}
	 
	 
	 
 }
}